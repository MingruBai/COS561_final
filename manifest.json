{

  "manifest_version": 2,
  "name": "Network Analytics Suite",
  "version": "1.0",
  "author": "Mingru Bai, Lawrence Lin, Qipeng Liu, Liwei Song",

  "background": {
      "scripts": ["background.js"]
  },
  "browser_action": {
      "default_title": "COS 561",
      "default_popup": "options.html"
  },
  "permissions": ["storage", "webRequest", "notifications", "webNavigation", "webRequestBlocking"],
  "web_accessible_resources": ["resources/countryNames.json"],
  "content_security_policy": "script-src 'self' https://maps.googleapis.com; object-src 'self'"
}
